
@{
    ViewBag.Title = "Add";
}

<h2>Add</h2>
<form action="@Url.Action("Add")" method="post" class="form-horizontal">
   
    <div class="form-group">
        <label class="control-label" for="RealmName">Realm Name</label>
        <input type="text" name="RealmName" id="RealmName" class="form-control" />
    </div>

    <div class="form-group">
        <label class="control-label" for="RealmCity">Realm City</label>
        <input type="text" name="RealmCity" id="RealmCity" class="form-control" />
    </div>

    <div class="form-group">
        <label class="control-label" for="RealmState">Realm State</label>
        <input type="text" name="RealmState" id="RealmState" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Save</button>
</form>

